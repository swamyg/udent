<div class="log-in">
	<%= form_for (@user) do |f| %>
	
	 
	    <%= f.label :name %>
	    <%= f.text_field :name %>
	  	<br />
	  	<br />
	    <%= f.label :email %>
	    <%= f.text_field :email %>
	  	<br />
	  	<br />
	    <%= f.label :password %>
	    <%= f.text_field :password %>
		<br />
	  	<br />
	    <%= f.label :professor %>
	    <%= f.check_box :professor %>
	 	<br />
	  	<br />
	    <%= f.submit "Register "%>
	
	<% end %>
</div>	
